<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="@color/primary"
    android:background="@color/secondary_m"

    android:orientation="vertical"
    tools:context=".ui.ActivityUserDetails">

    <RelativeLayout
        android:id="@+id/layLogout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginHorizontal="20dp"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:id="@+id/imgBack"
            android:layout_width="40dp"
            android:layout_height="40dp"
            app:cardCornerRadius="25dp"
            app:cardElevation="0dp"
            android:layout_alignParentRight="true"
            android:backgroundTint="@color/red"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:tint="@color/white"
                android:layout_gravity="center"

                android:src="@drawable/icon_logout"/>
        </androidx.cardview.widget.CardView>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="User Details"
            android:layout_marginLeft="10dp"
            android:layout_centerVertical="true"
            android:textColor="@color/black_m"
            android:textStyle="bold"
            android:textSize="20dp"/>

    </RelativeLayout>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="15dp"
        android:textColor="@color/lightGray"
        android:gravity="center_horizontal"
        android:layout_marginVertical="15dp"

        android:text="Please provide following information to continue the services of Life Changer (Pvt.) Ltd."/>

    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:shapeAppearanceOverlay="@style/LoginCardShape.CardView">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">




            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="5dp"
                android:textColor="@color/gray"
                android:gravity="center_horizontal"
                android:layout_marginVertical="10dp"
                android:textStyle="bold"
                android:layout_gravity="center_horizontal"
                android:fontFamily="@font/montserrat_extra_bold"
                android:text="Complete the following steps (7)"
                />

            <androidx.gridlayout.widget.GridLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                app:rowCount="1"
                app:columnCount="7"
                android:layout_gravity="center">


                <View
                    android:id="@+id/v1"
                    android:layout_width="0dp"
                    android:layout_height="3dp"
                    app:layout_columnWeight="1"
                    app:layout_row="0"
                    app:layout_column="0"
                    app:layout_rowWeight="1"
                    android:background="@color/textGrey"/>


                <View
                    android:id="@+id/v2"
                    android:layout_width="0dp"
                    android:layout_height="3dp"
                    app:layout_columnWeight="1"
                    app:layout_rowWeight="1"
                    app:layout_row="0"
                    android:layout_marginLeft="3dp"
                    app:layout_column="1"
                    android:background="@color/textGrey"/>


                <View
                    android:id="@+id/v3"

                    android:layout_width="0dp"
                    android:layout_height="3dp"
                    app:layout_columnWeight="1"
                    app:layout_rowWeight="1"
                    app:layout_row="0"
                    android:layout_marginLeft="3dp"
                    app:layout_column="2"
                    android:background="@color/textGrey"/>
                <View
                    android:id="@+id/v4"
                    android:layout_width="0dp"
                    android:layout_height="3dp"
                    app:layout_columnWeight="1"
                    app:layout_rowWeight="1"
                    app:layout_row="0"
                    android:layout_marginLeft="3dp"
                    app:layout_column="3"
                    android:background="@color/textGrey"/>
                <View
                    android:id="@+id/v5"
                    android:layout_width="0dp"
                    android:layout_height="3dp"
                    app:layout_columnWeight="1"
                    app:layout_rowWeight="1"
                    app:layout_row="0"
                    android:layout_marginLeft="3dp"
                    app:layout_column="4"
                    android:background="@color/textGrey"/>
                <View
                    android:id="@+id/v6"
                    android:layout_width="0dp"
                    android:layout_height="3dp"
                    app:layout_columnWeight="1"
                    app:layout_rowWeight="1"
                    app:layout_row="0"
                    android:layout_marginLeft="3dp"
                    app:layout_column="5"
                    android:background="@color/textGrey"/>
                <View
                    android:id="@+id/v7"
                    android:layout_width="0dp"
                    android:layout_height="3dp"
                    app:layout_columnWeight="1"
                    app:layout_rowWeight="1"
                    app:layout_row="0"
                    android:layout_marginLeft="3dp"
                    app:layout_column="6"
                    android:background="@color/textGrey"/>

            </androidx.gridlayout.widget.GridLayout>




            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">



                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="15dp"
                        app:cardElevation="6dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginBottom="3dp"
                        app:cardCornerRadius="8dp">


                        <RelativeLayout

                            android:id="@+id/layInvestorPhone"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/secondary_m">

                            <ImageView
                                android:layout_width="34dp"
                                android:layout_height="34dp"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="12dp"
                                android:src="@drawable/ic_phone"/>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="60dp"
                                android:orientation="vertical">
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/primary"
                                        android:textStyle="bold"
                                        android:layout_gravity="center_horizontal"
                                        android:text="Add Phone Number"/>


                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:src="@drawable/baseline_keyboard_arrow_right_24"
                                        android:tint="@color/primary" />
                                </LinearLayout>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Add and verify your phone number!"
                                    android:textSize="12dp"
                                    android:layout_marginTop="2dp"
                                    android:textColor="#7C7B7B" />

                                <TextView
                                    android:id="@+id/tvHeaderUserPhoneNumber"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:text="Required*"
                                    android:textColor="@color/red"
                                    android:textSize="10sp"
                                    android:textStyle="bold" />

                            </LinearLayout>


                            <ImageView
                                android:id="@+id/imgCheckUserPhoneNumber"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginVertical="25dp"
                                android:layout_marginRight="26dp"
                                android:src="@drawable/check_dark" />

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="15dp"
                        app:cardElevation="6dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="3dp"
                        app:cardCornerRadius="8dp">


                        <RelativeLayout
                            android:id="@+id/layInvestorBank"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/secondary_m">


                            <ImageView
                                android:layout_width="34dp"
                                android:layout_height="34dp"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="12dp"
                                android:src="@drawable/ic_bank"/>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="60dp"
                                android:orientation="vertical">
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/primary"
                                        android:textStyle="bold"
                                        android:layout_gravity="center_horizontal"
                                        android:text="Bank Account Details"/>


                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:src="@drawable/baseline_keyboard_arrow_right_24"
                                        android:tint="@color/primary" />
                                </LinearLayout>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Add bank account for funds transfer"
                                    android:textSize="12dp"
                                    android:layout_marginTop="2dp"
                                    android:textColor="#7C7B7B" />

                                <TextView
                                    android:id="@+id/tvHeaderUserBank"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:text="Required*"
                                    android:textColor="@color/red"
                                    android:textSize="10sp"
                                    android:textStyle="bold" />

                            </LinearLayout>


                            <ImageView
                                android:id="@+id/imgCheckUserBank"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginVertical="25dp"
                                android:layout_marginRight="26dp"
                                android:src="@drawable/check_dark" />

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="15dp"
                        app:cardElevation="6dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="3dp"
                        app:cardCornerRadius="8dp">


                        <RelativeLayout
                            android:id="@+id/layInvestorProfilePhoto"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/secondary_m">



                            <ImageView
                                android:layout_width="34dp"
                                android:layout_height="34dp"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="12dp"
                                android:src="@drawable/ic_profile"/>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="60dp"
                                android:orientation="vertical">
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/primary"
                                        android:textStyle="bold"
                                        android:layout_gravity="center_horizontal"
                                        android:text="Profile Photo"/>


                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:src="@drawable/baseline_keyboard_arrow_right_24"
                                        android:tint="@color/primary" />
                                </LinearLayout>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Upload your profile photo for identity"
                                    android:textSize="12dp"
                                    android:layout_marginTop="2dp"
                                    android:textColor="#7C7B7B" />

                                <TextView
                                    android:id="@+id/tvHeaderUserPhoto"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:text="Required*"
                                    android:textColor="@color/red"
                                    android:textSize="10sp"
                                    android:textStyle="bold" />

                            </LinearLayout>


                            <ImageView
                                android:id="@+id/imgCheckUserPhoto"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginVertical="25dp"
                                android:layout_marginRight="26dp"
                                android:src="@drawable/check_dark" />

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="15dp"
                        app:cardElevation="6dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="3dp"
                        app:cardCornerRadius="8dp">


                        <RelativeLayout
                            android:id="@+id/layInvestorCNIC"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/secondary_m">



                            <ImageView
                                android:layout_width="34dp"
                                android:layout_height="34dp"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="12dp"
                                android:src="@drawable/ic_cnic"/>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="60dp"
                                android:orientation="vertical">
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/primary"
                                        android:textStyle="bold"
                                        android:layout_gravity="center_horizontal"
                                        android:text="Add CNIC"/>


                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:src="@drawable/baseline_keyboard_arrow_right_24"
                                        android:tint="@color/primary" />
                                </LinearLayout>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Upload both side photo of your CNIC"
                                    android:textSize="12dp"
                                    android:layout_marginTop="2dp"
                                    android:textColor="#7C7B7B" />

                                <TextView
                                    android:id="@+id/tvHeaderUserCnic"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:text="Required*"
                                    android:textColor="@color/red"
                                    android:textSize="10sp"
                                    android:textStyle="bold" />

                            </LinearLayout>


                            <ImageView
                                android:id="@+id/imgCheckUserCnic"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginVertical="25dp"
                                android:layout_marginRight="26dp"
                                android:src="@drawable/check_dark" />

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="15dp"
                        app:cardElevation="6dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="3dp"
                        app:cardCornerRadius="8dp">


                        <RelativeLayout
                            android:id="@+id/layAddNominee"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/secondary_m">



                            <ImageView
                                android:layout_width="34dp"
                                android:layout_height="34dp"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="12dp"
                                android:src="@drawable/ic_nominee"/>
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="60dp"
                                android:orientation="vertical">
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/primary"
                                        android:textStyle="bold"
                                        android:layout_gravity="center_horizontal"
                                        android:text="Add Nominee"/>


                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:src="@drawable/baseline_keyboard_arrow_right_24"
                                        android:tint="@color/primary" />
                                </LinearLayout>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Nominate deserving individuals"
                                    android:textSize="12dp"
                                    android:layout_marginTop="2dp"
                                    android:textColor="#7C7B7B" />

                                <TextView
                                    android:id="@+id/tvHeaderNominee"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:text="Required*"
                                    android:textColor="@color/red"
                                    android:textSize="10sp"
                                    android:textStyle="bold" />

                            </LinearLayout>


                            <ImageView
                                android:id="@+id/imgCheckNominee"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginVertical="25dp"
                                android:layout_marginRight="26dp"
                                android:src="@drawable/check_dark" />

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="15dp"
                        app:cardElevation="6dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="3dp"
                        app:cardCornerRadius="8dp">


                        <RelativeLayout
                            android:id="@+id/layInvestorNomineeBank"

                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/secondary_m">


                            <ImageView
                                android:layout_width="34dp"
                                android:layout_height="34dp"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="12dp"
                                android:src="@drawable/ic_bank"/>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="60dp"
                                android:orientation="vertical">
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/primary"
                                        android:textStyle="bold"
                                        android:layout_gravity="center_horizontal"
                                        android:text="Nominee Bank Account"/>


                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:src="@drawable/baseline_keyboard_arrow_right_24"
                                        android:tint="@color/primary" />
                                </LinearLayout>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Bank account for funds claim"
                                    android:textSize="12dp"
                                    android:layout_marginTop="2dp"
                                    android:textColor="#7C7B7B" />

                                <TextView
                                    android:id="@+id/tvHeaderNomineeBank"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:text="Required*"
                                    android:textColor="@color/red"
                                    android:textSize="10sp"
                                    android:textStyle="bold" />

                            </LinearLayout>


                            <ImageView
                                android:id="@+id/imgCheckNomineeBank"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginVertical="25dp"
                                android:layout_marginRight="26dp"
                                android:src="@drawable/check_dark" />

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="15dp"
                        app:cardElevation="6dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        app:cardCornerRadius="8dp">


                        <RelativeLayout
                            android:id="@+id/layInvestorNomineeCNIC"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/secondary_m">


                            <ImageView
                                android:layout_width="34dp"
                                android:layout_height="34dp"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="12dp"
                                android:src="@drawable/ic_cnic"/>

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="60dp"
                                android:orientation="vertical">
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="@color/primary"
                                        android:textStyle="bold"
                                        android:layout_gravity="center_horizontal"
                                        android:text="Add Nominee CNIC"/>


                                    <ImageView
                                        android:layout_width="20dp"
                                        android:layout_height="20dp"
                                        android:src="@drawable/baseline_keyboard_arrow_right_24"
                                        android:tint="@color/primary" />
                                </LinearLayout>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Both sides photo of nominee CNIC"
                                    android:textSize="12dp"
                                    android:layout_marginTop="2dp"
                                    android:textColor="#7C7B7B" />

                                <TextView
                                    android:id="@+id/tvHeaderNomineeCnic"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="5dp"
                                    android:text="Required*"
                                    android:textColor="@color/red"
                                    android:textSize="10sp"
                                    android:textStyle="bold" />

                            </LinearLayout>


                            <ImageView
                                android:id="@+id/imgCheckNomineeCnic"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginVertical="25dp"
                                android:layout_marginRight="26dp"
                                android:src="@drawable/check_dark" />

                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <Button
                        android:id="@+id/btnStart"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_marginHorizontal="20dp"
                        android:background="@drawable/bg_button"
                        android:layout_marginBottom="20dp"
                        android:backgroundTint="@color/primary"
                        android:drawableRight="@drawable/baseline_keyboard_arrow_right_24"
                        android:drawableTint="@color/white"
                        android:text="Get Started" />


                </LinearLayout>

            </ScrollView>


        </LinearLayout>


    </com.google.android.material.card.MaterialCardView>



</LinearLayout>